<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mobilerto"
    tools:ignore="GoogleAppIndexingWarning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".activity.SignUpActivity" />
        <activity android:name=".activity.RedirectingHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.VehicleSearchActivity" />
        <activity android:name=".activity.LicenseSearchActivity" />
        <activity android:name=".activity.FileUploadActivity" />
        <activity android:name=".activity.ApplyLicenseActivity" />
        <activity android:name=".activity.ApplyRegistrationActivity" />
        <activity android:name=".activity.MockQuizActivity" />
    </application>
</manifest>